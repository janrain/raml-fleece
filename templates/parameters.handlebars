<dl>
{{#each params}}
  <dt>
    <span class="param">{{@key}}</span>
    {{#if type}}<span class="badge">{{type}}</span>{{/if}}
    {{#if required}}<span class="badge">required</span>{{/if}}
  </dt>
  <dd>
    {{#if description}}
      {{markdown description}}
    {{/if}}
    {{#if enum}}
      <b>Possible values: </b>
      <ul>
      {{#each enum}}
        <li><tt>{{this}}</tt></li>
      {{/each}}
      </ul>
    {{/if}}
    {{#if minimum}}
      <b>Minimum: </b>
      <code>{{minimum}}</code>
    {{/if}}
    {{#if maximum}}
      <b>Maximum: </b>
      <code>{{maximum}}</code>
    {{/if}}
    {{#if example}}
      <b>Example: </b>
      <code>{{example}}</code>
    {{/if}}
    {{#if default}}
      <b>Default: </b>
      <code>{{default}}</code>
    {{/if}}
  </dd>
{{/each}}
</dl>
